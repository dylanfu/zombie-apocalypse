    "@types/prettier": "2.6.3",
    "@types/sinon-chai": "^3.2.8",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "chai": "^4.3.6",
    "eslint": "^8.16.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "2.7.1",
    "sinon-chai": "^3.7.0",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.6.4"
